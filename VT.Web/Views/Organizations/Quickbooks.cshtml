@model VT.Web.Models.SplashMerchantModel

@{
    ViewBag.Title = "Quickbooks Settings";
    Layout = "~/Views/Shared/LoggedInLayout.cshtml";
}

@section PageLevelStyles{
    <link href="~/Content/plugins/switchery/switchery.css" rel="stylesheet" />
}

@section PageLevelScripts{
    @Scripts.Render("~/plugins/validate")
    @Scripts.Render("~/plugins/iCheck")
    <script src="~/Scripts/plugins/switchery/switchery.js"></script>
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h3><label style="color:#1ab394;"><b>QUICK BOOKS ONLINE INTEGRATION</b></label></h3>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="row">
                        @Html.HiddenFor(x => x.EntityLoginId)
                        @Html.HiddenFor(x => x.CompanyId)
                        <div class="hr-line-dashed"></div>
                        <div class="col-lg-12 max-container">
                            <div class="row">
                                <div class="col-lg-12">
                                    <label>Pull Services data from QB</label>                                    
                                    @Html.CheckBoxFor(x => x.IsAdmin, new { @class = "small js-switch" })                                
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <label>Push Services data to QB</label>
                                    @Html.CheckBoxFor(x => x.IsAdmin, new { @class = "small js-switch" })
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <label>Pull Customer data from QB</label>
                                    @Html.CheckBoxFor(x => x.IsAdmin, new { @class = "small js-switch" })
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <label>Push Customer data to QB</label>
                                    @Html.CheckBoxFor(x => x.IsAdmin, new { @class = "small js-switch" })
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <label>Pull Employee data from QB</label>
                                    @Html.CheckBoxFor(x => x.IsAdmin, new { @class = "small js-switch" })
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <label>Push Employee data to QB</label>
                                    @Html.CheckBoxFor(x => x.IsAdmin, new { @class = "small js-switch" })
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <label>Pull Customers data from QB</label>
                                    @Html.CheckBoxFor(x => x.IsAdmin, new { @class = "small js-switch" })
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <label>Copy Workflow Invoices to QB</label>
                                    @Html.CheckBoxFor(x => x.IsAdmin, new { @class = "small js-switch" })
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>